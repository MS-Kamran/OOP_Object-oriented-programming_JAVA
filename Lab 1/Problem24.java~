//PROBLEM 24) Palindrome//

import java.util.Scanner;

public class Problem24{
    public static void main(String [] args) {
        
        Scanner scan = new Scanner (System.in);
        System.out.println("Enter a Number:");
        int n = scan.nextInt();
        
        for (int i=1; i<=n ; i++)
        {
            System.out.print(i);
        }
        if (i==n)
        {
           for (int i=n; i>=n ; i--)               
               {
                System.out.print(i);
               } 
        }
        System.out.println();
    }        
}