public class ClockDemo13{
  public static void main(String[] args){
      
      Clock c1 = new Clock(10000);
      for (int i=0 ; i<=10 ; i++)
      {
          c1.tick();
          System.out.println(c1.toString());
      }
      
      Clock c2 = new Clock(5, 20, 45);
      for (int i=0 ; i<=10 ; i++)
      {
          c2.tick();
          System.out.println(c1.toString());
      }
  }
}